{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Rules": [
    {
      "Name": "master",
      "Match": "refs/heads/master",
      "Execute": "start_master.sh"
    },
    {
      "Name": "dev",
      "Match": "refs/heads/dev",
      "Execute": "start_dev.sh"
    }
  ]
}
